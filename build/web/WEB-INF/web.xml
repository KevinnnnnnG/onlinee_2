<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>controladorAu</servlet-name>
        <servlet-class>controlador.controladorAu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controladorCat</servlet-name>
        <servlet-class>controlador.controladorCat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controladorPro</servlet-name>
        <servlet-class>controlador.controladorPro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controladorU</servlet-name>
        <servlet-class>controlador.controladorU</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controladorOrd</servlet-name>
        <servlet-class>controlador.controladorOrd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidarID</servlet-name>
        <servlet-class>controlador.ValidarID</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>controladorAu</servlet-name>
        <url-pattern>/controladorAu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controladorCat</servlet-name>
        <url-pattern>/controladorCat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controladorPro</servlet-name>
        <url-pattern>/controladorPro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controladorU</servlet-name>
        <url-pattern>/controladorU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controladorOrd</servlet-name>
        <url-pattern>/controladorOrd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidarID</servlet-name>
        <url-pattern>/ValidarID</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
